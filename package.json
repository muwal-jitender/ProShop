    "url": "https://github.com/muwal-jitender/ProShop/issues"
  },
  "homepage": "https://github.com/muwal-jitender/ProShop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.3.5"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16"
  }
}
